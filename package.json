{
  "name": "g11n",
  "version": "3.0.4",
  "main": "dist/g11n.min.js",
  "scripts": {
    "build": "npm run clean && npm run build:umd && npm run build:umd:min",
    "build:dev": "NODE_ENV=production webpack src/G11N.js dist/G11N.js -w",
    "build:umd": "NODE_ENV=production webpack src/G11N.js dist/G11N.js",
    "build:umd:min": "NODE_ENV=production webpack src/G11N.js dist/G11N.min.js -p",
    "clean": "rimraf dist",
    "demo": "browser-sync start --serveStatic example dist",
    "lint": "eslint src",
    "test": "mocha --compilers js:babel-register --recursive",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/cswleocsw/g11n.git"
  },
  "license": "MIT",
  "dependencies": {
    "lodash.get": "^4.4.2",
    "whatwg-fetch": "^1.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.14.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "browser-sync": "^2.14.0",
    "chai": "^3.5.0",
    "log4js-free": "0.0.3",
    "mocha": "^3.0.2",
    "webpack": "^1.13.2"
  }
}
